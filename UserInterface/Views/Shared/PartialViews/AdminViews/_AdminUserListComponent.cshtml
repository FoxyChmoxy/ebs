@model  List<UserInterface.ViewModels.UserListViewModel>

<table class="table table-dark">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Email</th>
            <th scope="col">FullName</th>
            <th scope="col">Address</th>
            <th scope="col">Role</th>
        </tr>
    </thead>
    <tbody>
        @foreach (UserInterface.ViewModels.UserListViewModel user in Model)
        {
            <tr>
                <th scope="row">@user.Id</th>
                <td>@user.Email</td>
                <td>@user.FullName</td>
                <td>@user.Address</td>
                <td>@user.RoleName</td>
            </tr>
        }
    </tbody>
</table>